<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="DriverHostapp.Frontend.GeneralAppSettings.GeneralSettings"
        SizeToContent="WidthAndHeight"
        Title="GeneralSettings">
  <Border BorderThickness="10">
  <StackPanel>
    <TextBlock Name="Front_version"> </TextBlock>
    <TextBlock Name="Serial_version"> </TextBlock>
    <TextBlock Name="Bluetooth_version"> </TextBlock>
    <CheckBox Name="LoggingCheckbox" Checked="LoggingCheckBox_Checked" Unchecked="LoggingCheckBox_UnChecked">Enable Logging?</CheckBox>
  </StackPanel>
  </Border>
</Window>
